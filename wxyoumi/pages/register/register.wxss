  /*分隔块 start*/

  #interval-20{
    width:100%;
    height:20rpx;
    background-color:#F2F2F2;
  }

  /*分隔块 end*/

  /*重置 base.css 中的样式 start */
  input::-webkit-input-placeholder,textarea::-webkit-input-placeholder{
    color:#999;
  }
  input:-moz-placeholder,textarea:-moz-placeholder{
    color:#999;
  }
  /*重置 base.css 中的样式 end */