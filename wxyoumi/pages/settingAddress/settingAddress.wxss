 #warpper{
    background:#F7F7F7;
  }

  /*地址列表 start */
  .setting_address_list{
    width:750rpx;
    display: inline-block;
    background: #fff;
  }
  .setting_address_item{
    position: relative;
    width:620rpx;
    height:109rpx;
    padding:0rpx 65rpx;
    padding-top:18rpx;
    border-bottom: 1rpx solid #E7E7E7;
  }
  .setting_address_item text{
    color:#333;
    font-size:22rpx;
    line-height: 36rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
  }
  .editAddress{
    display: inline-block;
    position: absolute;
    right: 17rpx;
    top: 45rpx;
    width:34rpx;
    height:34rpx;
    background:url("http://m.youmias.com/static/images/userSetting/edit.png") no-repeat;
    background-size:100% 100%;
  }
  .checkAddress{
    position: absolute;
    top: 60rpx;
    left: 14rpx;
    width: 34rpx;
    height: 34rpx;
    display: inline-block;
    background: url("http://m.youmias.com/static/images/common/tick_gary.png") no-repeat;
    background-size: 100% 100%;
  }
  .checkAddress_active{
    background: url("http://m.youmias.com/static/images/common/tick_active.png") no-repeat;
    background-size: 100% 100%;
  }
  .add_address{
    width:720rpx;
    height:109rpx;
    padding:0rpx 15rpx;
    border-bottom: 1rpx solid #E7E7E7;
  }
  .add_address .addAddresBtn{
    float:left;
    line-height:109rpx;
    font-size:25rpx;
  }
  .add_address .addAddressTxt{
    float:right;
    display: inline-block;
    width:42rpx;
    height:42rpx;
    margin-top:34rpx;
    background:url("http://m.youmias.com/static/images/userSetting/add.png") no-repeat;
    background-size:100% 100%;
  }
  /*地址列表 end */

  /*选择地址弹出层 start */
  .address_option{
    width:750rpx;
    display: inline-block;
    position: fixed;
    bottom: 0;
    left: 0;
    background:#fff;
  }
  .address_option_title{
    width:100%;
    height:67rpx;
    background:#F3F2F8;
    padding:25rpx;
  }

  .address_option_title text{
    text-align:left;
    color:#363636;
    line-height: 67rpx;
  }
  .address_option_title text{
    display: inline-block;
    width:29rpx;
    height:29rpx;
    background:url("../../assets/img/userSetting/close.png") no-repeat;
    background-size: 100% 100%;
  }
  /*选择地址弹出层 end */

 