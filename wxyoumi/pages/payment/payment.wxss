#warpper{
    width:750rpx;
    margin:0rpx auto;
  }

  /*支付方式 start */
  .payment-types{
    width:750rpx;
    display: inline-block;
  }
  .payment-types ul{
    width:750rpx;
  }
  .payment-types ul li{
    position: relative;
    padding:21rpx 75rpx 30rpx 92rpx;
    width:582rpx;
    display: inline-block;
    border-bottom: 1rpx solid #dbdbdb;
  }
  .payment-team-logo{
    position: absolute;
    left:21rpx;
    top:31rpx;
    display: inline-block;
    width:42rpx;
    height:42rpx;
  }
  .payment-team-logo image{
    width:100%;
    height:100%;
  }
  .payment-types ul .payment-team-name{
    color:#242727;
    font-weight: bold;
    font-size:28rpx;
    display: block;
    text-align: left;
  }
  .payment-types ul .payment-team-explain{
    margin-top:5.04rpx;
    color:#9A9C9E;
    display: block;
    font-size:22rpx;
    text-align: left;
  }
  .payment-type-state{
    position: absolute;
    top: 40rpx;
    right: 21rpx;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    background:url("http://m.youmias.com/static/images/common/tick_gary.png") no-repeat;
    background-size:100% 100%;
  }
  .paymentTypeStateActive{
    background:url("http://m.youmias.com/static/images/common/tick_active.png") no-repeat;
    background-size:100% 100%;
  }

  /*支付方式 end */

  /*确认支付金额 start */
  .submit-payment{
    width:750rpx;
    height: 92rpx;
    background: #F23232;
    left: 0;
    right: 0;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
    text-align: center;
    color:#ffffff;
  }
  .submit-payment text{
    line-height: 92rpx;
    font-size:29rpx;
  }
  /*确认支付金额 end */
