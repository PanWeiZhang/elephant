/*详情 内容  start*/

  .product_details{
    width:730rpx;
    height:auto;
    padding:10rpx;
    padding-bottom:80rpx;
    display:inline-block;
    overflow:hidden;
  }
  .product_details image{
    width:100%;
    height:100%;
  }
  /*详情 内容  end*/
  /*产品详情底部nav start */
  .product-buy-nav{
    width:750rpx;
    height: 93rpx;
    background: #ffffff;
    left: 0;
    right: 0;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
  }
  .product-buy-nav ul{
    width:750rpx;
  }
  .product-buy-nav ul li{
    width: 25%;
    height:100%;
    float:left;
  }
  .product-buy-nav ul li text{
    color:#333;
  }
  .pro-save{
    width:100%;
    height:93rpx;
  }
  .pro-save text{
    text-align: center;
    display: block;
  }
  .pro-save image{
    margin:0rpx auto;
    display: block;
    width:45rpx;
    height:45rpx;
    margin-top: 17rpx;
  }
  .product-buy-nav ul li.addShoppingCar{
    background: #FFB042;
    text-align: center;
    font-size:27rpx;
    line-height:92rpx;
    color:#ffffff;
  }
  .product-buy-nav ul li.timelyBuy{
    font-size:27rpx;
    text-align: center;
    line-height:92rpx;
    background: #F23232;
    color:#ffffff;
  }
  .product-buy-nav ul li.addShoppingCar .addCarTxt{
    color:#fff;
  }  
  .product-buy-nav ul li.timelyBuy .buyTxt{
    color:#fff;
  } 
  /*产品详情底部nav end */

  /*分隔块 start*/

  .interval01{
    width:750rpx;
    height:20px;
    background-color:#F2F2F2;
  }

  /*分隔块 end*/