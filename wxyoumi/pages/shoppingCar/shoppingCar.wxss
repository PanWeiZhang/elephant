 #warpper{
    width:750rpx;
    margin:0rpx auto;
    overflow: hidden;
  }

   /*收藏总数量 start*/
  .collection-number{
    width:690rpx;
    height:75rpx;
    padding:0rpx 30rpx;
    position: relative;
  }
  .collection-number text{
    text-align: left;
    line-height: 75rpx;
    color:#969696;
  }
  .collection-number .cNum{
    color:#E43C3F;
  }
  .collection-number .edit-collection{
    width: 72rpx;
    height: 38rpx;
    position: absolute;
    right: 20rpx;
    top: 17rpx;
    font-size:26rpx;
    display: inline-block;
    line-height:38rpx;
  }
  /*收藏总数量 end*/

  /*购物车主体内容 start*/
  .shoppingCarBody{
    width:750rpx;
    display:inline-block;
    background-color:#F2F2F2;
  }
  .shoppingCarList{
    width:750rpx;
    display: inline-block;
    background-color: #fff;
  }
  .shopping-car-listItem{
    border-bottom: 1rpx solid #DDDDDD;
    position: relative;
    width:680rpx;
    display: inline-block;
    padding:25rpx 8.5rpx;
    padding-left:59rpx;
  }
  .shopping-car-itemImg{
    width:189rpx;
    height:189rpx;
    display: block;
    float:left;
  }
  .shopping-car-itemImg image{
    width:100%;
    height:100%;
  }
  .shopping-car-itemBaseInfo{
    width:465rpx;
    height:189rpx;
    float:left;
    margin-left:13rpx;
  }
  .carList-name{
    width:100%;
  }
  .carList-name text{
    color:#363636;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    font-size:24rpx;
  }
  .carList-order-info{
    width:100%;
    margin-top:13rpx;
  }
  .carList-order-info text{
    color:#99999B;
  }
  .carList-priceAndNumber{
    width:465rpx;
    height:46rpx;
    margin-top:25rpx;
    position: relative;
  }
  .carList-edit{
    position: absolute;
    width:126rpx;
    height:100%;
  }
  .carList-edit ul{
    display: inline-block;
    width:100%;
  }
  .carList-edit ul li{
    width:42rpx;
    height:42rpx;
    position: absolute;
  }
  .carList-edit ul li.carList-edit-shoucang{
    top:0;
    left:0;
    background:url("http://m.youmias.com/static/images/shoppingCar/buyActive.png") no-repeat;
    background-size:100% 100%;
  }
  .carList-edit ul li.edit-shoucang-active{
    background:url("http://m.youmias.com/static/images/shoppingCar/shoucang_active.png") no-repeat;
    background-size:100% 100%;
  }
  .carList-edit ul li.carList-edit-del{
    top:0;
    left:71rpx;
    background:url("http://m.youmias.com/static/images/shoppingCar/del.png") no-repeat;
    background-size:100% 100%;
  }
  .carList-edit ul li image{
    width:100%;
    height:100%;
  }
  .carList-price{
    display: block;
    width:100.8001rpx;
    position: absolute;
    color:#FF6600;
    font-size:28rpx;
  }
  .calcNmberCopy{
    display: inline-block;
    vertical-align: middle;
    border:1rpx solid #333;
    border-radius: 8rpx;
    float:right;
  }
  .calcNmberCopy text{
    color:#333;
    position: relative;
    float: left;
    width:50rpx;
    height:40rpx;
    line-height:50rpx;
    text-align: center;
  }

  .calcNmberCopy .decProductNumCopy{
    border-right-width:1rpx;
    border-right-style: solid;
    border-right-color: #333;
  }

  .calcNmberCopy .addProductNumCopy{
    border-left-width: 1rpx;
    border-left-style: solid;
    border-right-color: #333;
  }

  .calcNmberCopy .decProductNumCopy:after, .calcNmberCopy .addProductNumCopy:after, .calcNmberCopy .addProductNumCopy:before {
    position: absolute;
    left: 40%;
    top: 50%;
    content: "";
    width:21rpx;
    height:3rpx;
    margin: -1rpx 0 0 -1rpx;
    background: #000;
  }
  .calcNmberCopy .decProductTeShu{
    background: #f7f7f7;
    border-radius: 10rpx 0rpx 0rpx 10rpx;
  }
  .calcNmberCopy .addProductTeShu{
    background: #f7f7f7;
  }
  .calcNmberCopy .decProductTeShu:after{
    background: #ccc;
  }
  .calcNmberCopy .addProductTeShu:after{
    background: #ccc;
  }
  .calcNmberCopy .addProductTeShu:before{
    background: #ccc;
  }
  .calcNmberCopy .addProductNumCopy:before {
    width: 2rpx;
    height: 20rpx;
    margin: -10rpx 0 0 8rpx;
  }
  .calcNmberCopy .numberValueInputCopy{
    width: 71rpx;
    height:34rpx;
    float: left;
    border: 0;
    font-size:24rpx;
    text-align: center;
    background:#fff;
    min-height:40rpx;
  }
  .shopping-car-listActive{
    position: absolute;
    left:8.4rpx;
    top:115rpx;
    width:40rpx;
    height:40rpx;
    background:url("http://m.youmias.com/static/images/shoppingCar/ok.png") no-repeat;
    background-size:100% 100%;
  }
  .carListActiveActive{
    background:url("http://m.youmias.com/static/images/shoppingCar/ok_active.png") no-repeat;
    background-size:100% 100%;
  }
  /*购物车主体内容 end*/

  /*间隔样式 start */
  .interval-style-1{
    position: relative;
    width:100%;
    height:76rpx;
    background-color:#F2F2F2;
  }
  .interval-style-1 text{
    position: absolute;
    top:37rpx;
    left:0;
    width:100%;
    height:1rpx;
    background:#999;
    z-index: 10;
  }
  .interval-style-1 text{
    position: absolute;
    top:21rpx;
    left:218rpx;
    color:#999;
    z-index: 11;
    padding-left:25rpx;
    padding-right: 25rpx;
    background-color:#F2F2F2;
  }
  /*间隔样式 end */

  /*为您推荐 start*/
  .shoppingRecommend{
    display: inline-block;
    padding:0rpx 4.2rpx;
    padding-bottom: 84rpx;
    background-color:#F2F2F2;
    width:100%;
  }
  .pro-recommend-item{
    float:left;
    background:#fff;
    width:48%;
    margin-bottom: 13rpx
  }
  .pro-recommend-item:nth-child(odd){
    margin-right:2%;
  }
  .pro-recommend-image{
    width:100%;
    height:315rpx;
    margin-bottom:8.4rpx;
  }
  .pro-recommend-img image{
    width:355rpx;
    height:375rpx;
  }
  .pro-recommend-base-info{
    position: relative;
    color: #363636;
    padding:0rpx 13rpx;
    padding-bottom: 17rpx;
  }
  .pro-recommend-base-name{
    height:56rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
  }
  .pro-recommend-base-price{
    font-size:30rpx;
    margin-top:25.20002rpx;
    color:#FF6600;
  }
  .pro-recommend-aux{
    position: absolute;
    right:13rpx;
    bottom:12rpx;
    width:42rpx;
    height:42rpx;
    background: url("http://m.youmias.com/static/images/shoppingCar/car.png") no-repeat;
    background-size:100% 100%;
  }

  .proRecommendAuxActive{
    background: url("http://m.youmias.com/static/images/shoppingCar/car_active_red.png") no-repeat;
    background-size:100% 100%;
  }

  /*为您推荐 end*/

  /*提交订单 start*/
  .order-conmmit-payment{
    border-top:1rpx solid #E5E5E5;
    padding-right:210rpx;
    z-index:100;
    width:540rpx;
    height:84rpx;
    background: #ffffff;
    left: 0;
    right: 0;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
  }
  .order-conmmit-payment .totalPriceTxt{
    text-align: right;
    margin-right:21rpx;
    line-height: 84rpx;
    font-size:24rpx;
    color:#363636;
    padding-left:40rpx;
  }
  .order-conmmit-payment .totalMoney{
    color:#FF6600;
    font-size:30rpx;
  } 
  .order-conmmit-payment .order-conmmit-btn{
    color:#ffffff;
    line-height: 84rpx;
    text-align: center;
    font-size:25rpx;
    position: absolute;
    right:0rpx;
    top:0rpx;
    width:210rpx;
    height:84rpx;
    background:#EA3E40;
  }
  .order-conmmit-payment .allSelect{
    position: absolute;
    left: 20rpx;
    top: 0;
    width: 126rpx;
    height: 84rpx;
  }
  .order-conmmit-payment .all-select-pro{
    position: absolute;
    display: block;
    left: 0rpx;
    top: 20rpx;
    width: 40rpx;
    overflow: hidden;
    height: 40rpx;
    background: url("http://m.youmias.com/static/images/shoppingCar/ok.png") no-repeat;
    background-size: 100% 100%;
  }
  .order-conmmit-payment .allselectProActive{
    background: url("http://m.youmias.com/static/images/shoppingCar/ok_active.png") no-repeat;
    background-size: 100% 100%;
  }
  .order-conmmit-payment .conmmit-price{
    text-align:right;
    margin-right:30rpx;
    line-height:78rpx;
  }
  .order-conmmit-payment .allSelectTxt{
      margin-left:45rpx;
      line-height:78rpx;
  }
  /*提交订单 end*/

  /*分隔块 start*/

  .interval-20{
    width:100%;
    height:17rpx;
    background-color:#F2F2F2;
  }

  /*分隔块 end*/